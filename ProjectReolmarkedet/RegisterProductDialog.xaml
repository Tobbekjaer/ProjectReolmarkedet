        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjectReolmarkedet"
        mc:Ignorable="d"
        Title="RegisterProductDialog" Height="400" Width="600">
    <Grid>
        <Label Content="Vare:" HorizontalAlignment="Left" Margin="73,85,0,0" VerticalAlignment="Top"/>
        <Label Content="Pris:" HorizontalAlignment="Left" Margin="73,130,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="tbProduct" HorizontalAlignment="Left" Margin="133,89,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
